{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    { "src": "/server.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/req(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/search(.*)",
      "dest": "/server.js"
    }
  ]
}